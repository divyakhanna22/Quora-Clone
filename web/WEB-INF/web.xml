<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>
            loginpage.html
        </welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>newpackage.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogServlet</servlet-name>
        <servlet-class>newpackage.LogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JSPDemo</servlet-name>
        <servlet-class>newpackage.JSPDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginPageServlet</servlet-name>
        <servlet-class>Controller.LoginPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpPageServlet</servlet-name>
        <servlet-class>Controller.SignUpPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutPageServlet</servlet-name>
        <servlet-class>Controller.LogoutPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomePageServlet</servlet-name>
        <servlet-class>Controller.HomePageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddQuestionServlet</servlet-name>
        <servlet-class>Controller.AddQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnswerQuestionServlet</servlet-name>
        <servlet-class>Controller.AnswerQuestionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogServlet</servlet-name>
        <url-pattern>/LogServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JSPDemo</servlet-name>
        <url-pattern>/JSPDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginPageServlet</servlet-name>
        <url-pattern>/LoginPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpPageServlet</servlet-name>
        <url-pattern>/SignUpPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutPageServlet</servlet-name>
        <url-pattern>/LogoutPageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomePageServlet</servlet-name>
        <url-pattern>/HomePageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddQuestionServlet</servlet-name>
        <url-pattern>/AddQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnswerQuestionServlet</servlet-name>
        <url-pattern>/AnswerQuestionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
